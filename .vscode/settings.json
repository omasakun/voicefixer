{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "editor.formatOnSave": true,
  "[python]": {
    "editor.defaultFormatter": "eeyore.yapf"
  },
  "python.formatting.provider": "",
  "python.analysis.diagnosticMode": "workspace",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
  },
}
